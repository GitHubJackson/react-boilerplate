{
  "name": "new-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/react": "^6.18.2",
    "@sentry/tracing": "^6.18.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "ahooks": "^3.1.13",
    "antd": "^4.19.2",
    "axios": "^0.26.1",
    "customize-cra": "^1.0.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "dotenv -e env/.env.dev react-app-rewired start",
    "start:dev": "dotenv -e env/.env.dev react-app-rewired start",
    "start:test": "dotenv -e env/.env.test react-app-rewired start",
    "start:prod": "dotenv -e env/.env.prod react-app-rewired start",
    "build:dev": "dotenv -e env/.env.dev react-app-rewired build",
    "build:test": "dotenv -e env/.env.test react-app-rewired build",
    "build": "dotenv -e env/.env.prod react-app-rewired build",
    "build:prod": "dotenv -e env/.env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "mock": "nodemon src/mock/server.js",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "precommit": "eslint src/**/*"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "customize-cra-less-loader": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.2",
    "husky": "^7.0.4",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
